
using System.Threading.Channels;

namespace FirstProject
{

    class Program
    {
        static void Main(string[] args)
        {
            while (true)
            {
                Console.WriteLine("podaj liczbÄ™");
                string userInput = Console.ReadLine();
                int number = int.Parse(userInput);
                if (number == 0)
                {
                    break;
                }
                else if (number >= 1 && number <= 10) {
                    Console.WriteLine("Liczba w przedziale od 1 do 10");
                }
                else if (number >= 11 && number <= 100)
                {
                    Console.WriteLine("Liczba w przedziale od 11 do 100");
                }
                else if (number >= 101 && number <= 1000)
                {
                    Console.WriteLine("Liczba w przedziale od 101 do 1000");
                }
                else if (number >= 1001 && number <= 10000)
                {
                    Console.WriteLine("Liczba w przedziale od 1001 do 10000");
                }
                else if (number >= 10001 && number <= 100000)
                {
                    Console.WriteLine("Liczba w przedziale od 10001 do 100000");
                }
                else if (number >= 100001 && number <= 1000000)
                {
                    Console.WriteLine("Liczba w przedziale od 100001 do 1000000");
                }
                else if (number >= 1000001 && number <= 10000000)
                {
                    Console.WriteLine("Liczba w przedziale od 1000001 do 10000000");
                }
                else if (number >= 10000001 && number <= 100000000)
                {
                    Console.WriteLine("Liczba w przedziale od 10000001 do 100000000");
                }
                else if (number >= 100000001 && number <= 1000000000)
                {
                    Console.WriteLine("Liczba w przedziale od 100000001 do 1000000000");
                }
            }
        }
    }
}
